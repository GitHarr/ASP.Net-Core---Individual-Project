  </PropertyGroup>

  <ItemGroup>
    <None Remove="appsettingsDb.json" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="appsettingsDb.json">
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
