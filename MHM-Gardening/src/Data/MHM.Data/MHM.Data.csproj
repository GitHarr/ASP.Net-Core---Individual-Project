    <PackageReference Include="Microsoft.Extensions.Configuration.UserSecrets" Version="2.2.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\MHM.Data.Models\MHM.Data.Models.csproj" />
    <ProjectReference Include="..\MHMApp.Data.Common\MHMApp.Data.Common.csproj" />
  </ItemGroup>

</Project>
